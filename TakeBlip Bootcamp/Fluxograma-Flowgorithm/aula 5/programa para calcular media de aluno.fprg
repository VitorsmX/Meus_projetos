            <output expression="&quot;Informe seu nome completo: &quot;" newline="True"/>
            <input variable="nomeAluno"/>
            <output expression="&quot;Informe a nota da sua primeira avalia&#231;&#227;o: &quot;" newline="True"/>
            <input variable="notaAv1"/>
